<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
     <th scope="col">Item</th>
                    <th scope="col">Title</th>
                    <th scope="col">Quantity</th>
    </tr>
  </thead>
  <tbody>
      {{#each products}}
    <tr>
      
      <th scope="row">1</th>
      <td><img style="width: 70px; height:70px"
                src="/product-images/{{this._id}}.jpg" </td>
     <td>{{this.Name}}</td>

      <td>  <button class="cart-item-count mr-3">-</button>1<button class="cart-item-count ml-3">+</button></td>
       <td>
        <a href="" class="btn btn-danger">Remove</a>
        </td>
        
    </tr>
    {{/each}}
  </tbody>
</table>